<Window x:Class="BitMiracle.Docotic.Pdf.Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BitMiracle.Docotic.Pdf.Samples"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Print" HorizontalAlignment="Left" Margin="15,15,0,0" VerticalAlignment="Top" Width="130" Height="40" Click="Print_Click"/>
        <Button Content="Preview" HorizontalAlignment="Left" Margin="180,15,0,0" VerticalAlignment="Top" Width="130" Height="40" Click="Preview_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="15,77,0,0" VerticalAlignment="Top" Width="295" Height="25" Name="PrintSizeCombobox">
            <ComboBoxItem IsSelected="True">Fit page</ComboBoxItem>
            <ComboBoxItem>Actual Size</ComboBoxItem>
        </ComboBox>
    </Grid>
</Window>
