<Window x:Class="HtmlToPdfWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HtmlToPdfWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="215" Width="348">
    <Grid>
        <Label Content="URL to convert:" HorizontalAlignment="Left" Margin="35,32,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.043,0.24"/>
        <TextBox x:Name="textBoxUrl" HorizontalAlignment="Left" Margin="38,63,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="271"/>
        <Button x:Name="buttonConvert" Content="Convert" HorizontalAlignment="Left" Margin="38,101,0,0" VerticalAlignment="Top" Click="Button_Click" Width="136"/>
        <ProgressBar x:Name="progressBarConverting" HorizontalAlignment="Left" Height="20" Margin="179,101,0,0" VerticalAlignment="Top" Width="130" IsIndeterminate="True"/>

    </Grid>
</Window>
